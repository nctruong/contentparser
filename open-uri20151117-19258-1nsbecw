    <script type="text/javascript">
        //Set ScreenSizeCookie for Mobile Banner and set script on the head to avoid Google Tracking Script when reload.
        (function setScreenSizeCookie(){
            var width = window.screen.width;
            var height = window.screen.height;

            var screensize = (Math.sqrt(width * width + height * height)).toFixed(0);

            var cookieKey = 'device_screensize';
            var setCookie = function(name, value, path) {
                var cookiesTimeout = parseFloat(4) * 7;
                var d = new Date();
                d.setTime(d.getTime() + (cookiesTimeout * 24*60*60*1000));
                var expiresString = d.toUTCString();

                document.cookie = [
                    name + '=' + value,
                    'path=' + path,
                    'expires=' + expiresString
                ].join(';');
            };
            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for(var i=0; i<ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0)==' ') c = c.substring(1);
                    if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
                }
                return "";
            };

            var cookieValue = getCookie(cookieKey);

            if(!cookieValue){
                setCookie(cookieKey, screensize, '/');
            }

        })();
    </script>
